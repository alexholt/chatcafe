{
  "name": "chatcafe",
  "version": "1.0.0",
  "description": "Chat application",
  "main": "src/server/index.js",
  "scripts": {
    "start-client": "SERVER_URL=http://localhost:8000 NODE_ENV=development webpack-dev-server --hot --config webpack.config.js --colors --progress",
    "build": "NODE_ENV=production webpack --config webpack.config.js -p",
    "start-server": "CLIENT_URL=http://localhost:8080 nodemon src/server/index.js",
    "start-mongo": "mongod --dbpath=/data",
    "seed-mongo": "node src/server/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/alexholt/chatcafe"
  },
  "keywords": [
    "chat"
  ],
  "author": "Alex Holt",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.7",
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.2",
    "css-loader": "^0.25.0",
    "ejs": "^2.5.2",
    "ejs-loader": "^0.3.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "mongodb": "^2.2.10",
    "node-sass": "^3.10.0",
    "normalize.css": "^4.2.0",
    "postcss": "^5.2.0",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "svg-inline-loader": "^0.7.0",
    "svg-loader": "0.0.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "devDependencies": {
    "nodemon": "^1.10.2"
  }
}
